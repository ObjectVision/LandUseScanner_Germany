							    && Present/policy_maps/green_corridor > value(0, Policy_Levels), Label = "Arable area (alloc) in green corridors";
					attribute <YesOrNo> ArableInHighGreenCorridors     (domain_grid):
								=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/green_corridor > value(1, Policy_Levels), Label = "Arable area (alloc) in in green corridors with high policy level";
					attribute <YesOrNo> ArableInMineralPlanning     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/mineral_planning > value(0, Policy), Label = "Arable area (alloc) in policy areas with high policy level for mineral extraction";
					attribute <YesOrNo> ArableInHighMineralPlanning     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/mineral_planning > value(1, Policy), Label = "Arable area (alloc) in Planned areas for mineral extraction";
					attribute <YesOrNo> ArableInNatureLandscape     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/nature_landscape > value(0, Policy), Label = "Arable area (alloc) in Policy areas for Nature and Landscape";
					attribute <YesOrNo> ArableInHighNatureLandscape     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/nature_landscape > value(1, Policy), Label = "Arable area (alloc) in Policy areas for Nature and Landscape with high policy level";
					attribute <YesOrNo> ArableInGroundSurfaceWater     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/ground_surface_water > value(0, Policy), Label = "Arable area (alloc) in Ground surface water protection areas";
					attribute <YesOrNo> ArableInHighGroundSurfaceWater     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/ground_surface_water > value(1, Policy), Label = "Arable area (alloc) in Ground surface water protection areas with high policy level";
					attribute <YesOrNo> ArableInAgriculture     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/agriculture > value(0, Policy), Label = "Arable area (alloc) in Agriculture protection areas";
					attribute <YesOrNo> ArableInHighAgriculture     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/agriculture > value(1, Policy), Label = "Arable area (alloc) in Agriculture protection areas with high policy level";
					attribute <YesOrNo> ArableInForestry     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/forestry > value(0, Policy), Label = "Arable area (alloc) in Forestry protection areas";
					attribute <YesOrNo> ArableInHighForestry     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/forestry > value(1, Policy), Label = "Arable area (alloc) in Forestry protection areas with high policy level";

   					attribute <YesOrNo> ArableAllInHighPolicyAreas    (domain_grid):
						=ArableInHighFloodProtectedAreas

					attribute <YesOrNo> PastureInGroundSurfaceWater     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuPasture
							    && Present/policy_maps/ground_surface_water > value(0, Policy), Label = "Pasture area (alloc) in Ground surface water protection areas";
					attribute <YesOrNo> PastureInHighGroundSurfaceWater     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuPasture
							    && Present/policy_maps/ground_surface_water > value(1, Policy), Label = "Pasture area (alloc) in Ground surface water protection areas with high policy level";
					attribute <YesOrNo> PastureInAgriculture     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuPasture
							    && Present/policy_maps/agriculture > value(0, Policy), Label = "Pasture area (alloc) in Agriculture protection areas";
					attribute <YesOrNo> PastureInHighAgriculture     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuPasture
							    && Present/policy_maps/agriculture > value(1, Policy), Label = "Pasture area (alloc) in Agriculture protection areas with high policy level";
					attribute <YesOrNo> PastureInForestry     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuPasture
							    && Present/policy_maps/forestry > value(0, Policy), Label = "Pasture area (alloc) in Forestry protection areas";
					attribute <YesOrNo> PastureInHighForestry     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuPasture
							    && Present/policy_maps/forestry > value(1, Policy), Label = "Pasture area (alloc) in Forestry protection areas with high policy level";

   					attribute <YesOrNo> PastureAllInHighPolicyAreas    (domain_grid):
						=PastureInHighFloodProtectedAreas
							   ||PastureInHighGreenCorridors
							   ||PastureInHighMineralPlanning
							   ||PastureInHighNatureLandscape
							   ||PastureInHighGroundSurfaceWater
							   ||PastureInHighAgriculture
							   ||PastureInHighForestry > value(0, YesOrNo), Label = "Pasture area (alloc) in all policy areas with high policy level";

   					attribute <YesOrNo> PastureAllInPolicyAreas    (domain_grid):
						=PastureInHighFloodProtectedAreas || PastureInFloodProtectedAreas
							   ||PastureInHighGreenCorridors
							   ||PastureInGreenCorridors
							   ||PastureInHighMineralPlanning
							   ||PastureInMineralPlanning
							   ||PastureInHighNatureLandscape
							   ||PastureInNatureLandscape
							   ||PastureInHighGroundSurfaceWater
							   ||PastureInGroundSurfaceWater
							   ||PastureInHighAgriculture
							   ||PastureInAgriculture
							   ||PastureInHighForestry
							   ||PastureInForestry > value(0, YesOrNo), Label = "Pasture area (alloc) in all policy areas with high policy level";


				}
			}
	}	
	container Urban_Sprawl: Descr = "A set of indicators providing inside in the extent of urbanisation (spatial configuration indicators)"		
		{
			container Current: Descr = "urban sprawl indicators for 2009"
			{
				unit<uint32> UrbanAreas
				:	     = district(uint8(present/landuse/IsUrban))
				,	IsHidden = "True"
				{
					attribute<UrbanAreas> Districts (domain_grid);
					attribute<UrbanAreas> ID_Districts (domain_grid) := Districts *uint32(present/landuse/IsUrban);
					attribute<luHa>       Size      (UrbanAreas) := Float32(pcount(Districts)) * value(1, luHa);
					attribute<luHa> DistrictSize(domain_grid) := Size[Districts] * Float32(present/landuse/IsUrban);
					attribute<luHa> DistrictSizeGrTh1ha(domain_grid): 
									 =  (DistrictSize * float32(DistrictSize < 100000[luHa]))
											- (DistrictSize * float32(DistrictSize == 1[luHa]));
					attribute<luHa> NrUrbanAreasEq1ha(domain_grid) :=  DistrictSize * float32(DistrictSize == 1[luHa]);
				}
				parameter <luHa> AvgSizeUrbanAreas
				:	     = sum(UrbanAreas/Size* float32(UrbanAreas/Size <= 100000[luHa]))/sum(float32(UrbanAreas/Size <= 100000[luHa]));
				
				parameter <luHa> AvgSizeUrbanAreas1ha
				:	     = (sum(UrbanAreas/Size* float32(UrbanAreas/Size <= 100000[luHa]))-sum(UrbanAreas/Size* float32(UrbanAreas/Size == 1[luHa])))/(sum(float32(UrbanAreas/Size <= 100000[luHa]))-sum(float32(UrbanAreas/Size == 1[luHa])));

				parameter <float32> NoOfUrbanAreas1ha
				:	     = sum(float32(UrbanAreas/Size <= 100000[luHa]))-sum(float32(UrbanAreas/Size == 1[luHa]));
								
// Erläuterung zur AvgSizeUrbanAreas:
//	Vor dem Bruchstrich:	Die Größe der Patches (basierend auf der Tabelle Size) wird summiert, zunächst würde das für alle Patches gelten, also auch die 25 die nicht urban sind. Es sollen aber nur die Patches berücksichtigt werden, die kleiner als 100tsd ha sind, da das die Patches sind, die urban sind (alle größeren gehören nicht zu urban und sind die Restfläche der 25 Tiles).
//							Die Auswahl der eigentlichen Patches basiert auf einer Abfrage, aus der eine ja/nein Antwort hervorgeht, die durch Umwandlung in Float32 in 1/0 umgewandelt wird. Durch Multiplikation mit der Patchgröße erhält man die Gesamtfläche der urbanen Patches.
//	Nach dem Bruchstrich:	Ergibt die Anzahl der Patches: Wieder Abfrage nach Patches, die kleiner als 100tsd ha sind und Umwandlung der ja/nein Ergebnisse in 0/1. Diese für Konsistenz in Float32 umwandeln und die einsen aufsummieren. (Bei Verwendung von count kommt nichts sinnvolles raus).			

				attribute <luHa> AvgSizeUrbanAreasRegions1 (Geography/RegionDomains/Kreise) //bezieht sich auf die Patches > 1ha
				:	     = sum(UrbanAreas/DistrictSizeGrTh1ha, Geography/RegionGrids/Kreise)
							   / sum(float32(Bool(UrbanAreas/DistrictSizeGrTh1ha)), Geography/RegionGrids/Kreise);

				attribute <luHa> AvgSizeUrbanAreasRegions2 (Geography/RegionDomains/Kreise) //bezieht sich auf die Patches > 1ha
				:		 = (sum(UrbanAreas/DistrictSize * float32(UrbanAreas/DistrictSize < 100000[luHa]), Geography/RegionGrids/Kreise)
			    			   - sum(UrbanAreas/DistrictSize * float32(UrbanAreas/DistrictSize == 1[luHa]), Geography/RegionGrids/Kreise))
							   / (sum(Float32(UrbanAreas/DistrictSize < 100000[luHa]), Geography/RegionGrids/Kreise)
							   - sum(Float32(UrbanAreas/DistrictSize == 1[luHa]), Geography/RegionGrids/Kreise));

//Zwei Rechenwege, die zu unterschiedlichen Ergebnissen führen:	 Könnte der Grund darin liegen, wann das partitioning gemacht wird? Dadurch könnten in
//																 Regionen Patches mit einem ha Größe entstehen, die dann eleminiert werden, wenn die
//																 Größenabfrage auf den regionalisierten Daten läuft, nicht aber, wenn das Gesamtdeutsche^
//																 Grid verwendet wird.
//																 Für die Durchschnittliche Patchgröße ist Variante 2 sinnvoller
//																 In der Berechnung für die Anzahl der Patches kommt bei beiden Wegen nichts sinnvolles raua.

				attribute <float32> NrOfUrbanAreasPerRegion1 (Geography/RegionDomains/Kreise)  //bezieht sich auf die Patches > 1ha
				:	     = sum(float32(bool(UrbanAreas/DistrictSizeGrTh1ha)), Geography/RegionGrids/Kreise)/AvgSizeUrbanAreasRegions1;

				attribute <float32> NrOfUrbanAreasPerRegion2 (Geography/RegionDomains/Kreise): IsHidden = "True"  //bezieht sich auf die Patches > 1ha
				,	     = (sum(Float32(UrbanAreas/DistrictSize < 100000[luHa]), Geography/RegionGrids/Kreise)
							   - sum(Float32(UrbanAreas/DistrictSize == 1[luHa]), Geography/RegionGrids/Kreise))/AvgSizeUrbanAreasRegions2;

				attribute <float32> NrOfUrbanAreasPerRegion3 (Geography/RegionDomains/Kreise): IsHidden = "True"  //bezieht sich auf die Patches > 1ha
				,	     = sum(float32(bool(UrbanAreas/DistrictSizeGrTh1ha)), Geography/RegionGrids/Kreise)/AvgSizeUrbanAreasRegions2;
			}
				
			container Allocated: Descr = "urban sprawl indicators for allocation year"
			{
				unit<uint32> UrbanAreas
				:	     = district(uint8(results/landuse/AnderungenVorherigerZeitschritt/IsUrban))
				,	IsHidden = "True"
				{
					attribute<UrbanAreas> Districts (domain_grid);
					attribute<UrbanAreas> ID_Districts (domain_grid) := Districts *uint32(results/landuse/AnderungenVorherigerZeitschritt/IsUrban);
					attribute<luHa>       Size      (UrbanAreas) := Float32(pcount(Districts)) * value(1, luHa);
					attribute<luHa> DistrictSize(domain_grid) := Size[Districts] * Float32(results/landuse/AnderungenVorherigerZeitschritt/IsUrban); //Multiplikation notwendig, damit die Rasterkarte nicht in die 25 Tiles aufgeteilt bleibt und somit nicht jedes Tile ein District ohne Urban hat
					attribute<luHa> DistrictSizeGrTh1ha(domain_grid):
									 =  (DistrictSize * float32(DistrictSize < 100000[luHa]))
											- (DistrictSize * float32(DistrictSize == 1[luHa]));
					attribute<luHa> NrUrbanAreasEq1ha(domain_grid) :=  DistrictSize * float32(DistrictSize == 1[luHa]);
					
//					attribute<YesorNo>    IsBuiltup (UrbanAreas) := modus(results/landuse/AnderungenVorherigerZeitschritt/IsUrban, Districts); //verstehe den Zweck nicht, stamt aus LUS-Demo mit Contiuous Algorthismus
				}
				parameter <luHa> AvgSizeUrbanAreas:= sum(UrbanAreas/Size* float32(UrbanAreas/Size <= 100000[luHa]))/sum(float32(UrbanAreas/Size <= 100000[luHa]));
				
				parameter <luHa> AvgSizeUrbanAreas1ha
				:	     = (sum(UrbanAreas/Size* float32(UrbanAreas/Size <= 100000[luHa]))-sum(UrbanAreas/Size* float32(UrbanAreas/Size == 1[luHa])))/(sum(float32(UrbanAreas/Size <= 100000[luHa]))-sum(float32(UrbanAreas/Size == 1[luHa])));

				parameter <float32> NoOfUrbanAreas1ha
				:	     = (sum(float32(UrbanAreas/Size <= 100000[luHa]))-sum(float32(UrbanAreas/Size == 1[luHa])));

				attribute <luHa> AvgSizeUrbanAreasRegions1 (Geography/RegionDomains/Kreise) //bezieht sich auf die Patches > 1ha
				:	     = sum(UrbanAreas/DistrictSizeGrTh1ha, Geography/RegionGrids/Kreise)
							   / sum(float32(Bool(UrbanAreas/DistrictSizeGrTh1ha)), Geography/RegionGrids/Kreise);

				attribute <luHa> AvgSizeUrbanAreasRegions2 (Geography/RegionDomains/Kreise) //bezieht sich auf die Patches > 1ha
				:		 = (sum(UrbanAreas/DistrictSize * float32(UrbanAreas/DistrictSize < 100000[luHa]), Geography/RegionGrids/Kreise)
			    			   - sum(UrbanAreas/DistrictSize * float32(UrbanAreas/DistrictSize == 1[luHa]), Geography/RegionGrids/Kreise))
							   / (sum(Float32(UrbanAreas/DistrictSize < 100000[luHa]), Geography/RegionGrids/Kreise)
							   - sum(Float32(UrbanAreas/DistrictSize == 1[luHa]), Geography/RegionGrids/Kreise));

				attribute <float32> NrOfUrbanAreasPerRegion1 (Geography/RegionDomains/Kreise): IsHidden = "True"  //bezieht sich auf die Patches > 1ha
				,	     = sum(float32(bool(UrbanAreas/DistrictSizeGrTh1ha)), Geography/RegionGrids/Kreise)/AvgSizeUrbanAreasRegions1;

				attribute <float32> NrOfUrbanAreasPerRegion2 (Geography/RegionDomains/Kreise): IsHidden = "True"  //bezieht sich auf die Patches > 1ha
				,	     = (sum(Float32(UrbanAreas/DistrictSize < 100000[luHa]), Geography/RegionGrids/Kreise)
							   - sum(Float32(UrbanAreas/DistrictSize == 1[luHa]), Geography/RegionGrids/Kreise))/AvgSizeUrbanAreasRegions2;

				attribute <float32> NrOfUrbanAreasPerRegion3 (Geography/RegionDomains/Kreise): IsHidden = "True"  //bezieht sich auf die Patches > 1ha
				,	     = sum(float32(bool(UrbanAreas/DistrictSizeGrTh1ha)), Geography/RegionGrids/Kreise)/AvgSizeUrbanAreasRegions2;
				// Erläuterung zu NrOfUrbanAreasPerRegion: Das ist nur eine Annäherung, indem ich die Anzahl Zellen die Urban in einer Region sind gezählt habe und diese durch die Durchschnittliche Patchgröße der Region geteilt habe
			}
				
			container Change: Descr = "change of urban sprawl indicators since 2009"
			{
				parameter <float32> ChangeAvgPatchSize
				:			 =	((Allocated/AvgSizeUrbanAreas1ha / Current/AvgSizeUrbanAreas1ha)- value(1, float32))* value(100, float32);
				
				parameter <float32> ChangeNrOfUrbanAreas
				:			 =	((Allocated/NoOfUrbanAreas1ha / Current/NoOfUrbanAreas1ha)- value(1, float32))* value(100, float32);

				attribute <float32> ChangeAvgPatchSizePerRegion1 (Geography/RegionDomains/Kreise)
				:			 =	((Allocated/AvgSizeUrbanAreasRegions1 / Current/AvgSizeUrbanAreasRegions1)- value(1, float32))* value(100, float32);

				attribute <float32> ChangeAvgPatchSizePerRegion2 (Geography/RegionDomains/Kreise)
				:			 =	((Allocated/AvgSizeUrbanAreasRegions2 / Current/AvgSizeUrbanAreasRegions2)- value(1, float32))* value(100, float32);
				
				attribute <float32> ChangeNrOfUrbanAreasPerRegion1 (Geography/RegionDomains/Kreise)
				:			 =	((Allocated/NrOfUrbanAreasPerRegion1 / Current/NrOfUrbanAreasPerRegion1)- value(1, float32))* value(100, float32);
				
				attribute <float32> ChangeNrOfUrbanAreasPerRegion2 (Geography/RegionDomains/Kreise)
				:			 =	((Allocated/NrOfUrbanAreasPerRegion2 / Current/NrOfUrbanAreasPerRegion2)- value(1, float32))* value(100, float32);
				
				attribute <float32> ChangeNrOfUrbanAreasPerRegion3 (Geography/RegionDomains/Kreise)
				:			 =	((Allocated/NrOfUrbanAreasPerRegion3 / Current/NrOfUrbanAreasPerRegion3)- value(1, float32))* value(100, float32);
			}		
			
			
		}
	

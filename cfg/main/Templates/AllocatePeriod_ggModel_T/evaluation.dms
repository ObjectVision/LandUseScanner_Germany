		container DevArable_Areas: Descr = "Land use types (alloc) in policy-sensitive areas"
			{
				container Nature_protection: Descr = "Other land use types (alloc) in nature protection policy areas"
				{
					attribute <YesOrNo> ArableInNatureReserves     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/nature_reserves > value(0, Policy), Label = "Arable area (alloc) in Nature reserves";
					attribute <YesOrNo> ArableInNationalParks     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/national_parks > value(0, Policy), Label = "Arable area (alloc) in National Parks";
					attribute <YesOrNo> ArableInFFH     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/ffh > value(0, Policy), Label = "Arable area (alloc) in Flora-Fauna-Habitat areas";
					attribute <YesOrNo> ArableInSPA     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/spa > value(0, Policy), Label = "Arable area (alloc) in Bird protection areas";
					attribute <YesOrNo> ArableInRamsar     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/ramsar > value(0, Policy), Label = "Arable area (alloc) in Ramsar areas";
					attribute <YesOrNo> ArableInBiosphereCore     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/biosphere_reserves_core > value(0, Policy), Label = "Arable area (alloc) in Core areas of Biosphere reserves";
					attribute <YesOrNo> ArableInBiosphereDev     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/biosphere_reserves_development > value(0, Policy), Label = "Arable area (alloc) in Development areas of Biosphere reserves";
					attribute <YesOrNo> ArableInLSG     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/policy_maps/lsg > value(0, Policy), Label = "Arable area (alloc) in Landscape protection areas";

					attribute <YesOrNo> ArableAllInNatureProtectioAreas    (domain_grid):
						=ArableInNatureReserves
							   ||ArableInNationalParks
							   ||ArableInFFH
							   ||ArableInSpa
							   ||ArableInRamsar
							   ||ArableInBiosphereCore
							   ||ArableInBiosphereDev > value(0, YesOrNo), Label = "Arable area (alloc) in all nature protection policy areas";
				}
				container NatureLandscape: Descr = "Arable area (alloc) in Nature and Landscape areas (non policy areas)"
				{
					attribute <YesOrNo> ArableInUnfragmentedSpace     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/thematic_maps/NatureLandscape/UnfragmentedSpace > value(0, Policy), Label = "Arable area (alloc) in Unfragmented Spaces";
					attribute <YesOrNo> ArableInSoilQuality     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/thematic_maps/NatureLandscape/SoilQuality >= value(60, msqr), Label = "Arable area (alloc) in areas with high soil quality";
					attribute <YesOrNo> ArableInFreeLand     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/landuse/IsFreeLand > value(0, YesOrNo), Label = "Arable area (alloc) in Free Land";
					attribute <YesOrNo> ArableInHQ100     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/thematic_maps/NatureLandscape/Floods100y > value(0, Uint2), Label = "Arable area (alloc) in flooded areas";
					attribute <YesOrNo> CurrArableInHQ100     (domain_grid):
						=   present/landuse/IsArable
							    && Present/thematic_maps/NatureLandscape/Floods100y > value(0, Uint2), Label = "Arable area (alloc) in flooded areas";
					attribute <YesOrNo> CurrArableInLandslides     (domain_grid):
						=   results/landuse/IsArable
							    && Present/thematic_maps/NatureLandscape/Landslides > value(3, Landslide), Label = "Urban area in landslide areas (High and Very high)";
					attribute <YesOrNo> ArableInLandslides     (domain_grid):
						=   results/landuse/AnderungenSeit2022/ZuArable
							    && Present/thematic_maps/NatureLandscape/Landslides > value(3, Landslide), Label = "Urban area in landslide areas (High and Very high)";

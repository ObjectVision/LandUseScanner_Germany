container evaluation: Descr = "This container contains several evaluation indicators for simulation results"
{
	container Total
	{
		container TotalOrgLanduse:=
			for_each_nedv(
				  ModelType/Name
				, 'sum(
					  UInt32(Prev_landuse == value('+String(ID(ModelType))+', ModelType)) 
				  )'
				, void, UInt32
			)
		,	Descr = "Total current land use";

		container TotalMinClaims := 
			for_each_nedv(
				  AllocType/Name
				, 'sum(CaseData/MinClaims/'+AllocType/Name+')'
				, void, UInt32
			)
		,	Descr = "Total Minimm Claims";

		container TotalMaxClaims := 
			for_each_nedv(
				  AllocType/Name
				, 'sum(CaseData/MaxClaims/'+AllocType/Name+')'
				, void, UInt32
			)
		,	Descr = "Total Maxmum Claims";

		container AllocatedLanduse := 
			for_each_nedv(
				  AllocType/Name
				, 'sum(
					  UInt32(Endogenous == value('+String(ID(AllocType))+', AllocType)) 
				  )'
				, void, UInt32
			)
		,	Descr = "Total landuse after allocation";

		container MinClaimRealisation := 
			for_each_nedv(
				  AllocType/Name
				, 'Float32(AllocatedLanduse/'+AllocType/Name+') / Float32(TotalMinClaims/'+AllocType/Name+')'
				, void, Float32)
		,	Descr = "Degree to which initial demand is allocated";
		
		container MaxClaimRealisation := 
			for_each_nedv(
				  AllocType/Name
				, 'Float32(AllocatedLanduse/'+AllocType/Name+') / Float32(TotalMaxClaims/'+AllocType/Name+')'
				, void, Float32)
		,	Descr = "Degree to which initial demand is allocated";
	}
	container Regional
	{
		container Kreise
		{
			container OrgLanduse := 
				for_each_nedv(
					  ModelType/Name
					, 'sum(
						  UInt32(present/landuse/germany == value('+String(ID(ModelType))+', ModelType)) 
						, Geography/RegionGrids/Kreise
					  )'
					, Geography/RegionDomains/Kreise
					, UInt32
				)
			,	Descr = "Total current land use";
	
			container MinClaims := 
				for_each_nednv(
					  AllocType/Name
					, 'CaseData/MinClaims/'+AllocType/Name
					, Claims
					, AllocType/Name+'/RegionUnit'
					,UInt32
				)
			,	Descr = "Total minimum land demand";
	
			container MaxClaims := 
				for_each_nednv(
					  AllocType/Name
					, 'CaseData/MaxClaims/'+AllocType/Name
					, Claims
					, AllocType/Name+'/RegionUnit'
					,UInt32
				)
			,	Descr = "Total maximum land demand";
	
			container AllocatedLanduse := 
				for_each_nedv(
					  AllocType/Name
					, 'sum(
						  UInt32(Endogenous == value('+String(ID(AllocType))+', AllocType)) 
						, Geography/RegionGrids/Kreise
					  )'
					, Geography/RegionDomains/Kreise
					, UInt32)
			,	Descr = "Total allocated land use";
			
			container UrbanArea: Descr = "Changes in Urban Area"
			{
				attribute <float32> UrbanArea2022 (Geography/RegionDomains/Kreise):=	sum(float32(Present/landuse/IsUrban), Geography/RegionGrids/Kreise);
				attribute <float32> UrbanAreaModel (Geography/RegionDomains/Kreise):=	sum(float32(results/LandUse/AnderungenVorherigerZeitschritt/IsUrban), Geography/RegionGrids/Kreise);
				attribute <float32> UrbanGrowthAbsolute (Geography/RegionDomains/Kreise):=	UrbanAreaModel - UrbanArea2022;
				attribute <float32> UrbanGrowthRelative (Geography/RegionDomains/Kreise):=	UrbanGrowthAbsolute / UrbanArea2022 * value(100, float32);
			
				container FromTo : Descr = "Changes from one land-use class to another" 
				{		
					attribute <float32> ArableToUrban (Geography/RegionDomains/Kreise)
					:			 =	sum(float32(results/landuse/AnderungenSeit2022/ZuUrban && results/landuse/AnderungenSeit2022/VonArable), Geography/RegionGrids/Kreise);
					
					attribute <float32> PastureToUrban (Geography/RegionDomains/Kreise)
					:			 =	sum(float32(results/landuse/AnderungenSeit2022/ZuUrban && results/landuse/AnderungenSeit2022/VonPasture), Geography/RegionGrids/Kreise);
						
					attribute <float32> AgricultureToUrban (Geography/RegionDomains/Kreise)
					:			 =	sum(float32(results/landuse/AnderungenSeit2022/ZuUrban && results/landuse/AnderungenSeit2022/VonAgriculture), Geography/RegionGrids/Kreise);
	
					attribute <float32> ArableToUrbanPercent (Geography/RegionDomains/Kreise)
					:			 =	ArableToUrban / float32(results/LandUse/evaluation/Regional/OrgLanduse/arable);
					
					attribute <float32> PastureToUrbanPercent (Geography/RegionDomains/Kreise)
					:			 =	PastureToUrban / float32(results/LandUse/evaluation/Regional/OrgLanduse/pasture);
					
					attribute <float32> AgricultureToUrbanPercent (Geography/RegionDomains/Kreise)
					:			 =	PastureToUrban / float32(results/LandUse/evaluation/Regional/OrgLanduse/arable + results/LandUse/evaluation/Regional/OrgLanduse/pasture);
				}
			}

			container MinClaimRealisation := 
				for_each_nedv(
					  AllocType/Name
					, 'Float32(AllocatedLanduse/'+AllocType/Name+') / Float32(MinClaims/'+AllocType/Name+')'
					, Geography/RegionDomains/Kreise
					, ratio)
			,	Descr = "Degree to which initial demand is allocated";
			
			container MaxClaimRealisation := 
				for_each_nedv(
					  AllocType/Name
					, 'Float32(AllocatedLanduse/'+AllocType/Name+') / Float32(MaxClaims/'+AllocType/Name+')'
					, Geography/RegionDomains/Kreise
					, ratio)
			,	Descr = "Degree to which initial demand is allocated";
		}
		
		container Gemeinden
		{
			container OrgLanduse := 
				for_each_nedv(
					  ModelType/Name
					, 'sum(
						  UInt32(present/landuse/germany == value('+String(ID(ModelType))+', ModelType)) 
						, Geography/RegionGrids/Gemeinden
					  )'
					, Geography/RegionDomains/Gemeinden
					, UInt32
				)
			,	Descr = "Total current land use";

			container AllocatedLanduse := 
				for_each_nedv(
					  AllocType/Name
					, 'sum(
						  UInt32(Endogenous == value('+String(ID(AllocType))+', AllocType)) 
						, Geography/RegionGrids/Gemeinden
					  )'
					, Geography/RegionDomains/Gemeinden
					, UInt32
				), Descr = "Total allocated land use", StorageName = "='%LocalDataProjDir%/Results/AllocatedLU_perGemeinde_'+Scenario_name+'_'+Year_name+'.csv'", StorageType = "gdalwrite.vect"
			{
				attribute<string>  Gemeinde_code (Geography/RegionDomains/Gemeinden) := string(Geography/RegionDomains/Gemeinden/value);
			}
	
			container ChangeLanduse := 
				for_each_nedv(
					  AllocType/Name
					, '(Float32(Gemeinden/AllocatedLanduse/'+AllocType/Name+') / Float32(Gemeinden/OrgLanduse/'+AllocType/Name+') - float32(1)) * float32(100)'
					, Geography/RegionDomains/Gemeinden
					, ratio)
			,	Descr = "Percent change in land use for municipalities";
		}
		
		container reg_count
		{    
			container units   
			{      
				unit<uint32> Domain: nrofrows = 10;      
				unit<uint8>  RegioA: nrofrows = 5;      
				unit<uint8>  RegioB: nrofrows  = 3;      
				unit<uint8>  Class:  nrofrows = 4      
				{         
					attribute<string> label: ['Class1','Class2','Class3','Class4'];
		        }   
		     }   
		     container regios   
		     {      
			     attribute<units/RegioA> A (units/Domain):[0,0,1,2,3,0,4,2,1,0];      
			     attribute<units/RegioB> B (units/Domain):[1,0,2,2,0,0,1,2,1,0];      
			 }   
			 attribute<units/Class> src  (units/Domain): [0,1,3,2,0,1,2,1,0,3];   
			 attribute<string>      refs (units/Class) : ['A','A','B','A'];      
			 
			 container attributes := reg_count(         
			 							    src         
			 							  , units/Class/label         
			 							  , Regios         
			 							  , refs     
			 							  );
		}

	}
	container Local
	{
		container DifferenceMaps := 
			for_each_nedv(
				  ModelType/Name,
				' Int8(Model      == value('+String(ID(ModelType))+', ModelType)) 
				- Int8(present/landuse/germany == value('+String(ID(ModelType))+', ModelType))'
				, domain_grid, Int8
			)
		,	Descr = "Difference allocation - current landuse";
		
		container Current: Descr = "Analysis of current situation on local level in policy-sensitive areas"
		{
			container Urban: Descr = "Situation in urban land use types"
			{
				container Nature_Protection: Descr = "Current urban area in nature protection areas"
				{
					attribute <YesOrNo> CurrUrbanInNatureReserves     (domain_grid) :=    present/landuse/IsUrban 
							    && Present/policy_maps/nature_reserves > value(0, Policy), Label = "Urban area (alloc) in Nature reserves";
					attribute <YesOrNo> CurrUrbanInNationalParks     (domain_grid): 
						  =    present/landuse/IsUrban 
							    && Present/policy_maps/national_parks > value(0, Policy), Label = "Urban area (alloc) in National Parks";
					attribute <YesOrNo> CurrUrbanInFFH     (domain_grid): 
						  =    present/landuse/IsUrban 
							    && Present/policy_maps/ffh > value(0, Policy), Label = "Urban area (alloc) in Flora-Fauna-Habitat areas";
					attribute <YesOrNo> CurrUrbanInSPA     (domain_grid): 
						  =    present/landuse/IsUrban 
							    && Present/policy_maps/spa > value(0, Policy), Label = "Urban area (alloc) in Bird protection areas";
					attribute <YesOrNo> CurrUrbanInRamsar     (domain_grid): 
						  =    present/landuse/IsUrban 
							    && Present/policy_maps/ramsar > value(0, Policy), Label = "Urban area (alloc) in Ramsar areas";
					attribute <YesOrNo> CurrUrbanInBiosphereCore     (domain_grid): 
						  =    present/landuse/IsUrban 
							    && Present/policy_maps/biosphere_reserves_core > value(0, Policy_Levels), Label = "Urban area (alloc) in Core areas of Biosphere reserves";
					attribute <YesOrNo> CurrUrbanInBiosphereDev     (domain_grid): 
						  =    present/landuse/IsUrban 
							    && Present/policy_maps/biosphere_reserves_development > value(0, Policy), Label = "Urban area (alloc) in Development areas of Biosphere reserves";
					attribute <YesOrNo> CurrUrbanInLSG     (domain_grid): 
						  =    present/landuse/IsUrban 
							    && Present/policy_maps/lsg > value(0, Policy), Label = "Urban area (alloc) in Landscape protection areas";
							    
					attribute <YesOrNo> CurrUrbanAllInNatureProtectionAreas    (domain_grid): 
						 = CurrUrbanInNatureReserves 
							   || CurrUrbanInNationalParks
							   || CurrUrbanInFFH
							   || CurrUrbanInSpa
							   || CurrUrbanInRamsar
							   || CurrUrbanInBiosphereCore
							   || CurrUrbanInBiosphereDev > value(0, YesOrNo), Label = "Current urban area in all nature protection policy areas";
					
				    attribute <YesOrNo> CurrUrbanAllInNatureProtectionAreasAll    (domain_grid):
						 = CurrUrbanInNatureReserves 
							   || CurrUrbanInNationalParks
							   || CurrUrbanInFFH
							   || CurrUrbanInSpa
							   || CurrUrbanInRamsar
							   || CurrUrbanInLSG
							   || CurrUrbanInBiosphereCore
							   || CurrUrbanInBiosphereDev > value(0, YesOrNo), Label = "Current Urban area in all nature protection policy areas incl. LSG";
							   
					attribute <YesOrNo> CurrUrbanAllInNatureProtectionAreasHigh    (domain_grid):
						=CurrUrbanInNatureReserves 
							   ||CurrUrbanInNationalParks
							   ||CurrUrbanInFFH
							   ||CurrUrbanInSpa
							   ||CurrUrbanInBiosphereCore, Label = "Current urban area in all nature protection policy areas with high status";
							   
					attribute <YesOrNo> CurrUrbanAllInNatureProtectionAreasLow    (domain_grid):
						=CurrUrbanInRamsar
							   ||CurrUrbanInLSG
							   ||CurrUrbanInBiosphereDev > value(0, YesOrNo), Label = "Current urban area in all nature protection policy areas with low status";
							   
				}
				container Nature_Landscape: Descr = "Current urban area in nature and landscape areas (non-policy)"
				{
					attribute <YesOrNo> CurrUrbanInUnfragmentedSpace     (domain_grid):=   present/landuse/IsUrban
							    && Present/thematic_maps/NatureLandscape/UnfragmentedSpace > value(0, YesOrNo), Label = "Urban area in Unfragmented Spaces";
					attribute <YesOrNo> CurrUrbanInSoilQuality     (domain_grid):=   present/landuse/IsUrban 
							    && Present/thematic_maps/NatureLandscape/SoilQuality >= value(60, msqr), Label = "Urban area in areas with high soil quality";
					attribute <YesOrNo> CurrUrbanInHQ100     (domain_grid):=   present/landuse/IsUrban 
							    && Present/thematic_maps/NatureLandscape/Floods100y > value(0, Uint2), Label = "Urban area in flooded areas";
					attribute <YesOrNo> CurrUrbanInHQ200     (domain_grid):=   present/landuse/IsUrban 
							    && Present/thematic_maps/NatureLandscape/Floods_hwrmrl_sim > value(0, Uint2), Label = "Urban area in flooded areas (HQ200)";
					attribute <YesOrNo> CurrUrbanInHQextr     (domain_grid):=   present/landuse/IsUrban 
							    && Present/thematic_maps/NatureLandscape/Floods_extr > value(0, Uint2), Label = "Urban area in flooded areas (HQ200)";
					attribute <YesOrNo> CurrUrbanInLandslides     (domain_grid):=   present/landuse/IsUrban 
							    && Present/thematic_maps/NatureLandscape/Landslides > value(3, Landslide), Label = "Urban area in landslide areas (High and Very high)";
				}
				container Policy_Areas: Descr = "Current urban area in policy areas"
				{
					attribute <YesOrNo> CurrUrbanInFloodProtectedAreas     (domain_grid):=   present/landuse/IsUrban && Present/policy_maps/flood_protection > value(0, Policy_Levels), Label = "Urban area in flood protection areas";
					attribute <YesOrNo> CurrUrbanInSettlement     (domain_grid):=   present/landuse/IsUrban&& Present/policy_maps/settlement > value(0, Policy), Label = "Urban within Settlement areas";
				}
				container Restriction_Areas: Descr = "Current urban area in restricted areas"
				{
					attribute <YesOrNo> CurrUrbanInBufferWEA     (domain_grid):=   present/landuse/IsUrban && Present/restriction_maps/buffer_wea > value(0, Policy_Levels), Label = "Urban within the buffer of wind power stations";
				}
			}
			
			container Urban_oUG: Descr = "Situation in urban land use types"
			{
				container Nature_Protection: Descr = "Current urban area (_oUG) in nature protection areas"
				{
					attribute <YesOrNo> CurrUrban_oUGInNatureReserves     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/policy_maps/nature_reserves > value(0, Policy), Label = "Urban area (_oUG) in Nature reserves";
					attribute <YesOrNo> CurrUrban_oUGInNationalParks     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/policy_maps/national_parks > value(0, Policy), Label = "Urban area (_oUG) in National Parks";
					attribute <YesOrNo> CurrUrban_oUGInFFH     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/policy_maps/ffh > value(0, Policy), Label = "Urban area (_oUG) in Flora-Fauna-Habitat areas";
					attribute <YesOrNo> CurrUrban_oUGInSPA     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/policy_maps/spa > value(0, Policy), Label = "Urban area (_oUG) in Bird protection areas";
					attribute <YesOrNo> CurrUrban_oUGInRamsar     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/policy_maps/ramsar > value(0, Policy), Label = "Urban area (_oUG) in Ramsar areas";
					attribute <YesOrNo> CurrUrban_oUGInBiosphereCore     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/policy_maps/biosphere_reserves_core > value(0, Policy), Label = "Urban area (_oUG) in Core areas of Biosphere reserves";
					attribute <YesOrNo> CurrUrban_oUGInBiosphereDev     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/policy_maps/biosphere_reserves_development > value(0, Policy), Label = "Urban area (_oUG) in Development areas of Biosphere reserves";
					attribute <YesOrNo> CurrUrban_oUGInLSG     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/policy_maps/lsg > value(0, Policy), Label = "Urban area (_oUG) in Landscape protection areas";
							    
					attribute <YesOrNo> CurrUrban_oUGAllInNatureProtectionAreas    (domain_grid):
						=CurrUrban_oUGInNatureReserves 
							   ||CurrUrban_oUGInNationalParks
							   ||CurrUrban_oUGInFFH
							   ||CurrUrban_oUGInSpa
							   ||CurrUrban_oUGInRamsar
							   ||CurrUrban_oUGInBiosphereCore
							   ||CurrUrban_oUGInBiosphereDev > value(0, YesOrNo), Label = "Urban area (_oUG) in all nature protection policy areas";
					
					attribute <YesOrNo> CurrUrban_oUGAllInNatureProtectionAreasAll    (domain_grid):
						=CurrUrban_oUGInNatureReserves 
							   ||CurrUrban_oUGInNationalParks
							   ||CurrUrban_oUGInFFH
							   ||CurrUrban_oUGInSpa
							   ||CurrUrban_oUGInRamsar
							   ||CurrUrban_oUGInLSG
							   ||CurrUrban_oUGInBiosphereCore
							   ||CurrUrban_oUGInBiosphereDev > value(0, YesOrNo), Label = "Current Urban area (_oUG) in all nature protection policy areas incl. LSG";
							   
					attribute <YesOrNo> CurrUrban_oUGAllInNatureProtectionAreasHigh    (domain_grid):
						=CurrUrban_oUGInNatureReserves 
							   ||CurrUrban_oUGInNationalParks
							   ||CurrUrban_oUGInFFH
							   ||CurrUrban_oUGInSpa
							   ||CurrUrban_oUGInBiosphereCore, Label = "Current urban area (_oUG) in all nature protection policy areas with high status";
							   
					attribute <YesOrNo> CurrUrban_oUGAllInNatureProtectionAreasLow    (domain_grid):
						=CurrUrban_oUGInRamsar
							   ||CurrUrban_oUGInLSG
							   ||CurrUrban_oUGInBiosphereDev > value(0, YesOrNo), Label = "Current urban area (_oUG) in all nature protection policy areas with low status";
				}
				container Nature_Landscape: Descr = "Current urban area in nature and landscape areas (non-policy)"
				{
					attribute <YesOrNo> CurrUrban_oUGInUnfragmentedSpace     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/thematic_maps/NatureLandscape/UnfragmentedSpace > value(0, YesOrNo), Label = "Urban area (_oUG) in Unfragmented Spaces";
					attribute <YesOrNo> CurrUrban_oUGInSoilQuality     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/thematic_maps/NatureLandscape/SoilQuality >= value(60, msqr), Label = "Urban area (_oUG) in areas with high soil quality";
					attribute <YesOrNo> CurrUrban_oUGInHQ100     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/thematic_maps/NatureLandscape/Floods100y > value(0, Uint2), Label = "Urban area (_oUG) in flooded areas (JRC-Layer)";
					attribute <YesOrNo> CurrUrban_oUGInHQ_extr_bafg     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/thematic_maps/NatureLandscape/Floods_extr_bafg > value(0, Uint2), Label = "Urban area (_oUG) in flooded areas (BaFG-Layer)";
					attribute <YesOrNo> CurrUrban_oUGInHQ_combined     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/thematic_maps/NatureLandscape/Floods_combined > value(0, Uint2), Label = "Urban area (_oUG) in flooded areas (combined Layers)";
					attribute <YesOrNo> CurrUrban_oUGInBothHQ_combined     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/thematic_maps/NatureLandscape/Floods_combined == value(2, Uint2), Label = "Urban area (_oUG) in flooded areas (combined Layers)";
					attribute <YesOrNo> CurrUrban_oUGInHQ_L     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/thematic_maps/NatureLandscape/Floods_hwrmrl_sim > value(0, Uint2), Label = "Urban area (_oUG) in flooded areas (HQ200)";
					attribute <YesOrNo> CurrUrban_oUGInHQ_M     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/thematic_maps/NatureLandscape/Floods_hwrmrl_sim == value(2, Uint2), Label = "Urban area (_oUG) in flooded areas (HQ100)";
					attribute <YesOrNo> CurrUrban_oUGInHQ_extr     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/thematic_maps/NatureLandscape/Floods_extr > value(0, Uint2), Label = "Urban area (_oUG) in flooded areas (HQ200)";
					attribute <YesOrNo> CurrUrbanInLandslides_oUG     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/thematic_maps/NatureLandscape/Landslides > value(3, Landslide), Label = "Urban area in landslide areas (High and Very high)";
				}
				container Policy_Areas: Descr = "Current urban area in policy areas"
				{
					attribute <YesOrNo> CurrUrban_oUGInFloodProtectedAreas     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/policy_maps/flood_protection > value(0, Policy_Levels), Label = "Urban area (_oUG) in flood protection areas";
					attribute <YesOrNo> CurrUrban_oUGInHighFloodProtectedAreas     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/policy_maps/flood_protection == value(2, Policy_Levels), Label = "Urban area (_oUG) in high flood protection areas";
					attribute <YesOrNo> CurrUrban_oUGInSettlement     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/policy_maps/settlement > value(0, Policy), Label = "Urban area (_oUG) within Settlement areas";
				}
				container Restriction_Areas: Descr = "Current urban area (_oUG) in restricted areas"
				{
					attribute <YesOrNo> CurrUrban_oUGInBufferWEA     (domain_grid):
						=   present/landuse/IsUrban_oUG 
							    && Present/restriction_maps/buffer_wea > value(0, Policy_Levels), Label = "Urban area (_oUG) within the buffer of wind power stations";

				}
			}
			
			container UrbanFabric: Descr = "Situation in urban land use types"
			{
				container Nature_Protection: Descr = "Current urban fabric in nature protection areas"
				{
					attribute <YesOrNo> CurrUrbanFabricInNatureReserves     (domain_grid):
						=   present/landuse/germany == value (1, ModelType) 
							    && Present/policy_maps/nature_reserves > value(0, Policy), Label = "Current Urban fabric in Nature reserves";
					attribute <YesOrNo> CurrUrbanFabricInNationalParks     (domain_grid):
						=   present/landuse/germany == value (1, ModelType) 
							    && Present/policy_maps/national_parks > value(0, Policy), Label = "Current Urban fabric in National Parks";
					attribute <YesOrNo> CurrUrbanFabricInFFH     (domain_grid):
						=   present/landuse/germany == value (1, ModelType) 
							    && Present/policy_maps/ffh > value(0, Policy), Label = "Current Urban fabric in Flora-Fauna-Habitat areas";
